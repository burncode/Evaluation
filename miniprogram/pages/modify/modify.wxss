.container{
	width: 100%;
	padding: 0;
	margin: 0;
	display: flex;
	flex-direction: column;
}
.title {
  width: 100%;
  display: flex;
	font-size: 14pt;
  justify-content: center;
}
.tuijian{
	display: flex;
	justify-content: space-between;
	font-size: 11pt;
}
.text{
	padding-left: 15rpx;
	padding-top: 20rpx;
	color: #444;
}
.text::before{
	content:"*";
	color: red;
}
.img{
	margin: 50rpx 50rpx;
}
